<script>

    function validateEmail(email) {
        if (email !== '') {
            var pattern = /^([a-z0-9_\.-])+@[a-z0-9-]+\.([a-z]{2,4}\.)?[a-z]{2,4}$/i;
            if (pattern.test(email)) {
                return email;
            } else {
                swal({
                    text: "Вы ввели некорректный адрес электронной почты",
                    className: "swal-error"
                });
                $('.swal-title').css('color', 'black');
                return false;
            }
        } else {
            swal({
                text: "Пожалуйста, введите электронную почту"
            });
            $('.swal-title').css('color', 'black');
            return false;
        }
    }


    function sendContactForm() {
        var email = validateEmail($('#fMail').val());
        if (email){
            sendMessageContacts();
        }
    }

    function HideCaptchaAndForm() {
        $(".footer-form-wrap").hide();
        $("#contactsForm").hide();
    }

    function sendMessageContacts() {
        var xhr = new XMLHttpRequest();
        var name = document.getElementById('fName').value;
        var email = document.getElementById('fMail').value;
        var message = document.getElementById('fMessage').value;
        var captcha = grecaptcha.getResponse();
        var params = 'name=' + encodeURIComponent(name) +
            '&email=' + encodeURIComponent(email) +
            '&captcha=' + encodeURIComponent(captcha) +
            '&message=' + encodeURIComponent(message) +
            '&subject=' + encodeURIComponent('(Footer .RU)');

        xhr.open("POST", '/templates/mail.php', true);
        xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
        xhr.onreadystatechange = function () {
            if (xhr.status == 200) {
                if (xhr.responseText == 'ok') {
                    document.getElementById('message').innerHTML = 'Сообщение отправлено. Мы ответим Вам в течение 24 часов.';
                    HideCaptchaAndForm();
                } else {

                }
            }
        };
        xhr.send(params);
    }
