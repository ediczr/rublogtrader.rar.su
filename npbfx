<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script language="JavaScript" type="text/javascript">
/*<![CDATA[*/

document.onkeydown = checkEscape;
document.onkeypress = checkEscape;
function checkEscape(e) {
	if(!e) e = event;
	if (e.keyCode == 27) 
    return false;
}
var page = new PushPage();
page.context.setDebugAlertsOnClientError(true);
page.context.setDomain("npbfx-official.com");
page.onEngineCreation = function(engine){
	engine.connection.setLSHost("ls-quotes.npbfx-official.com");
	engine.connection.setLSPort(3120);
	engine.connection.setAdapterName("NEFTEPROM");
    engine.changeStatus("STREAMING");
}
page.bind();
page.createEngine("StocksApp", "https://www.npbfx-official.com/wp-content/themes/npbfxinfo/ls/", "SHARE_SESSION", true);
	
$(function(){
	//var currencies = ["EURUSD","USDJPY","GBPUSD","EURJPY","EURGBP","EURCHF","GBPJPY","GBPCHF","NZDUSD","AUDUSD","USDCAD"];
	$('select#quotes_select').on('change', function(){
		var type = parseInt($(this).val());
		$('.quotes-tab').eq(type - 1).addClass('active').siblings().removeClass('active');
		/*
		$('.quotes_table').html('<tr><th></th><th>Bid</th><th>Ask</th><th>Спред</th></tr>');
		currencies.forEach(function(item,i,arr){
			$('.quotes_table').append('<tr><td class="quotes_market">'+item+'</td><td class="quotes_data"><div source="lightstreamer" table="stockstable" item="info'+type+':'+item+'" field="bid">-</div></td><td class="quotes_data"><div source="lightstreamer" table="stockstable" item="info'+type+':'+item+'" field="offer">-</div></td><td class="quotes_data"> <div source="lightstreamer" table="stockstable" item="info'+type+':'+item+'" field="spread">-</div></td></tr>');			
		});
		*/
		//loadTable();
	})
});
function loadTable() {
	var table = new OverwriteTable(null, null, "MERGE");
	table.setDataAdapter("STOCKS");
	table.setSnapshotRequired(true);
	table.onItemUpdate = updateItem;
	table.onChangingValues = changingValues;
	table.setRequestedMaxFrequency(1.0);
	page.addTable(table, "stockstable");
}
loadTable();
function updateItem(item, updateInfo){
	if(updateInfo != null) {
    	updateInfo.addField("#bid_state", getFieldState("bid", updateInfo), true);
        updateInfo.addField("#offer_state", getFieldState("offer", updateInfo), true);
	}
}
function changingValues(item, itemUpdate){
	if(itemUpdate != null) {
    	itemUpdate.setHotTime(1000);
        itemUpdate.setHotToColdTime(200);
		updateFieldColor("bid", itemUpdate);
        updateFieldColor("offer", itemUpdate);
	}
}
function updateFieldColor(fieldName, itemUpdate) {
	var fieldState = itemUpdate.getServerValue("#" + fieldName + "_state");
    if(fieldState != "NOT_CHANGED") {
    	var hotColor = (fieldState == "UP") ? "#009b7a" : "#f58426";
        var coldColor = (fieldState == "UP") ? "#009b7a" : "#f58426";
        var hotBackgroundColor = "";
        var coldBackgroundColor = "";
        itemUpdate.setAttribute(fieldName, hotColor, coldColor, "color");
        itemUpdate.setAttribute(fieldName, hotBackgroundColor, coldBackgroundColor, "backgroundColor");
	}
}
function getFieldState(fieldName, updateInfo) {
	var state = "NOT_CHANGED";
    var oldValue = updateInfo.getOldValue(fieldName);
    if(oldValue == null) {
    	state = "UP";
	}
	else if (updateInfo.isValueChanged(fieldName)) {
    	state = (oldValue > updateInfo.getNewValue(fieldName)) ? "DOWN" : "UP";
	}
    return state;
}
/*]]>*/
</script>
